{
    "name": "labstag/core",
    "description": "Labstag Core",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "1.5.*",
        "doctrine/annotations": "2.0.*",
        "doctrine/dbal": "3.8.*",
        "doctrine/doctrine-bundle": "2.13.*",
        "doctrine/doctrine-migrations-bundle": "3.3.*",
        "easycorp/easyadmin-bundle": "4.12.*",
        "fakerphp/faker": "1.23.*",
        "gedmo/doctrine-extensions": "3.16.*",
        "giggsey/libphonenumber-for-php": "8.13.*",
        "knplabs/knp-menu-bundle": "3.4.*",
        "knplabs/knp-paginator-bundle": "6.5.*",
        "league/flysystem-bundle": "3.3.*",
        "liip/imagine-bundle": "^2.13",
        "nelmio/cors-bundle": "2.5.*",
        "spipu/html2pdf": "5.2.*",
        "symfony/apache-pack": "1.0.*",
        "symfony/console": "7.1.*",
        "symfony/doctrine-messenger": "7.1.*",
        "symfony/dotenv": "7.1.*",
        "symfony/flex": "2.4.*",
        "symfony/framework-bundle": "7.1.*",
        "symfony/mailer": "7.1.*",
        "symfony/notifier": "7.1.*",
        "symfony/rate-limiter": "7.1.*",
        "symfony/runtime": "7.1.*",
        "symfony/webpack-encore-bundle": "^2.2",
        "symfony/workflow": "7.1.*",
        "symfony/yaml": "7.1.*",
        "twig/extra-bundle": "3.13.*",
        "twig/intl-extra": "3.13.*",
        "vich/uploader-bundle": "2.4.*"
    },
    "config": {
        "bin-dir": "bin/",
        "optimize-autoloader": true,
        "preferred-install": {
          "*": "dist"
        },
        "platform": {
          "php": "8.3"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "Labstag\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Labstag\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "rector": "rector process --clear-cache",
        "rector-dry": "rector process --clear-cache --dry-run",
        "phpmnd": "phpmnd src features/bootstrap tests",
        "simple-phpunit": "simple-phpunit --verbose",
        "simple-phpunit-unit-integration": "simple-phpunit --testsuite unit,integration --verbose",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.6",
        "phpstan/phpdoc-parser": "1.32.*",
        "phpstan/phpstan": "1.12.*",
        "phpstan/phpstan-deprecation-rules": "1.2.*",
        "phpstan/phpstan-doctrine": "1.5.*",
        "phpstan/phpstan-mockery": "1.1.*",
        "phpstan/phpstan-phpunit": "1.4.*",
        "phpstan/phpstan-symfony": "1.4.*",
        "phpunit/phpunit": "11.4.*",
        "povils/phpmnd": "3.5.*",
        "rector/rector": "1.2.*",
        "symfony/browser-kit": "7.1.*",
        "symfony/css-selector": "7.1.*",
        "symfony/debug-bundle": "7.1.*",
        "symfony/maker-bundle": "1.61.*",
        "symfony/phpunit-bridge": "7.1.*",
        "symfony/stopwatch": "7.1.*",
        "symfony/web-profiler-bundle": "7.1.*",
        "symplify/phpstan-rules": "13.0.*"
    }
}
