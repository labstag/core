{
    "name": "labstag/core",
    "description": "Labstag Core",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "1.5.*",
        "doctrine/annotations": "2.0.*",
        "doctrine/doctrine-bundle": "2.13.*",
        "doctrine/doctrine-migrations-bundle": "3.4.*",
        "dompdf/dompdf": "3.1.*",
        "easycorp/easyadmin-bundle": "4.24.*",
        "essence/essence": "3.5.*",
        "gedmo/doctrine-extensions": "3.18.*",
        "giggsey/libphonenumber-for-php": "8.13.*",
        "knplabs/knp-menu-bundle": "3.5.*",
        "knplabs/knp-paginator-bundle": "6.6.*",
        "league/flysystem-bundle": "3.4.*",
        "liip/imagine-bundle": "2.13.*",
        "matomo/device-detector": "6.4.*",
        "nelmio/cors-bundle": "2.5.*",
        "nesbot/carbon": "3.8.*",
        "phpoffice/phpspreadsheet": "4.0.*",
        "phpoffice/phpword": "1.3.*",
        "symfony/apache-pack": "1.0.*",
        "symfony/console": "7.2.*",
        "symfony/doctrine-messenger": "7.2.*",
        "symfony/dotenv": "7.2.*",
        "symfony/event-dispatcher": "7.2.*",
        "symfony/flex": "2.4.*",
        "symfony/framework-bundle": "7.2.*",
        "symfony/http-client": "7.2.*",
        "symfony/mailer": "7.2.*",
        "symfony/monolog-bundle": "3.10.*",
        "symfony/notifier": "7.2.*",
        "symfony/rate-limiter": "7.2.*",
        "symfony/runtime": "7.2.*",
        "symfony/twig-bridge": "7.2.*",
        "symfony/twig-bundle": "7.2.*",
        "symfony/webpack-encore-bundle": "2.2.*",
        "symfony/workflow": "7.2.*",
        "symfony/yaml": "7.2.*",
        "twig/extra-bundle": "3.20.*",
        "twig/intl-extra": "3.20.*",
        "twig/twig": "3.20.*",
        "vich/uploader-bundle": "2.5.*"
    },
    "config": {
        "bin-dir": "bin/",
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "8.4"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "Labstag\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Labstag\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "ecs-check": "ecs check --ansi",
        "ecs-fix": "ecs check --fix --ansi",
        "rector": "rector process --clear-cache",
        "rector-dry": "rector process --clear-cache --dry-run",
        "phpmnd": "phpmnd config features public src tests",
        "simple-phpunit": "simple-phpunit --verbose",
        "simple-phpunit-unit-integration": "simple-phpunit --testsuite unit,integration --verbose",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        }
    },
    "require-dev": {
        "aalaap/faker-youtube": "2.0.*",
        "behat/behat": "3.19.*",
        "doctrine/doctrine-fixtures-bundle": "4.0.*",
        "fakerphp/faker": "1.24.*",
        "friendsofphp/php-cs-fixer": "3.69.*",
        "mmo/faker-images": "0.9.*",
        "phpmd/phpmd": "2.15.*",
        "phpstan/phpdoc-parser": "2.1.*",
        "phpstan/phpstan": "2.1.*",
        "phpstan/phpstan-deprecation-rules": "2.0.*",
        "phpstan/phpstan-doctrine": "2.0.*",
        "phpstan/phpstan-mockery": "2.0.*",
        "phpstan/phpstan-phpunit": "2.0.*",
        "phpstan/phpstan-symfony": "2.0.*",
        "phpunit/phpunit": "11.5.*",
        "povils/phpmnd": "3.5.*",
        "rector/rector": "2.0.*",
        "smknstd/fakerphp-picsum-images": "1.0.*",
        "squizlabs/php_codesniffer": "3.11.*",
        "symfony/browser-kit": "7.2.*",
        "symfony/css-selector": "7.2.*",
        "symfony/debug-bundle": "7.2.*",
        "symfony/maker-bundle": "1.62.*",
        "symfony/phpunit-bridge": "7.2.*",
        "symfony/stopwatch": "7.2.*",
        "symfony/web-profiler-bundle": "7.2.*",
        "symplify/easy-coding-standard": "12.5.*",
        "symplify/phpstan-rules": "14.3.*"
    }
}
