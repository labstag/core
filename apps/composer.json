{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.5",
        "doctrine/annotations": "^2.0",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.2",
        "easycorp/easyadmin-bundle": "^4.11",
        "fakerphp/faker": "^1.23",
        "gedmo/doctrine-extensions": "^3.16",
        "giggsey/libphonenumber-for-php": "^8.13",
        "knplabs/knp-menu-bundle": "^3.4",
        "knplabs/knp-paginator-bundle": "^6.4",
        "nelmio/cors-bundle": "^2.5",
        "symfony/apache-pack": "^1.0",
        "symfony/console": "7.1.*",
        "symfony/doctrine-messenger": "7.1.*",
        "symfony/dotenv": "7.1.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.1.*",
        "symfony/mime": "7.1.*",
        "symfony/notifier": "7.1.*",
        "symfony/rate-limiter": "7.1.*",
        "symfony/runtime": "7.1.*",
        "symfony/workflow": "7.1.*",
        "symfony/yaml": "7.1.*",
        "twig/extra-bundle": "^3.10",
        "twig/intl-extra": "^3.10"
    },
    "config": {
        "bin-dir": "bin/",
        "optimize-autoloader": true,
        "preferred-install": {
          "*": "dist"
        },
        "platform": {
          "php": "8.2"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "Labstag\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Labstag\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "rector": "rector process --clear-cache",
        "rector-dry": "rector process --clear-cache --dry-run",
        "phpmnd": "phpmnd src features/bootstrap tests",
        "simple-phpunit": "simple-phpunit --verbose",
        "simple-phpunit-unit-integration": "simple-phpunit --testsuite unit,integration --verbose",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.6",
        "phpstan/phpdoc-parser": "^1.30",
        "phpstan/phpstan": "^1.12",
        "phpstan/phpstan-deprecation-rules": "^1.2",
        "phpstan/phpstan-doctrine": "^1.5",
        "phpstan/phpstan-mockery": "^1.1",
        "phpstan/phpstan-phpunit": "^1.4",
        "phpstan/phpstan-symfony": "^1.4",
        "phpunit/phpunit": "^11.3",
        "povils/phpmnd": "^3.5",
        "rector/phpstan-rules": "^0.7.6",
        "rector/rector": "^1.2",
        "symfony/browser-kit": "7.1.*",
        "symfony/css-selector": "7.1.*",
        "symfony/debug-bundle": "7.1.*",
        "symfony/maker-bundle": "^1.60",
        "symfony/phpunit-bridge": "^7.1",
        "symfony/stopwatch": "7.1.*",
        "symfony/web-profiler-bundle": "7.1.*"
    }
}
