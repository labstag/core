<section id="{{ paragraph_id(paragraph) }}" class="{{ paragraph_class(paragraph) }}">
    <h2>{{ paragraph.title }}</h2>
    <div>
        <div>
            <ul>
                {% for row in histories %}
                    <li>
                        {% include  "teasers/history.html.twig"  with {'history': row} %}
                    </li>
                {% endfor %}
            </ul>
        </div>
        <div><a href="{{ path('front', {'slug': listing.slug}) }}" class="btn btn-primary">Voir toutes les histoires</a></div>
    </div>
</section>