<section id="{{ paragraph_id(paragraph) }}">
    <h2>{{ paragraph.title }}</h2>
    <ul>
        {% for post in news %}
            <li>
                {% include  "teasers/post.html.twig"  with {'post': post} %}
            </li>
        {% endfor %}
    </ul>
    <a href="{{ path('front', {'slug': listing.slug}) }}" class="btn btn-primary">{{ 'Read more' }}</a>
</section>