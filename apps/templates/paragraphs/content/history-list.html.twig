<section id="{{ paragraph_id(paragraph) }}" class="{{ paragraph_class(paragraph) }}">
    <div class="title">
        {{ paragraph.title }}
    </div>
    <ul>
        {% for row in pagination %}
            <li>
                {% include  "teasers/history.html.twig"  with {'history': row} %}
            </li>
        {% endfor %}
    </ul>
    <div class="navigation">
        {{ knp_pagination_render(pagination) }}
    </div>
</section>