{% use "elements.html.twig" %}
<section{% with {attr: paragraph_contextmenu(paragraph)} %}{{ block('attributes') }}{% endwith %}>
    <div class="wrapper">
        <h2>{{ paragraph.title }}</h2>
        <ul>
            {% for item in paragraph.competences %}
                <li class="teaser-competence">
                    {% include  "teasers/competences.html.twig"  with {'item': item} %}
                </li>
            {% endfor %}
        </ul>
    </div>
</section>