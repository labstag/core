{% use "elements.html.twig" %}
<section{% with {attr: paragraph_contextmenu(paragraph)} %}{{ block('attributes') }}{% endwith %}>
    <div class="background" style="--image-url: url('{{ site_asset(edito, 'img') }}')"></div>
    <div class="wrapper">
        <h2>{{ edito.title }}</h2>
        {% for tab in paragraphs %}
            {{ debug_begin(tab) }}
            {{ paragraph_show(tab) }}
            {{ debug_end(tab) }}
        {% endfor %}
    </div>
</section>