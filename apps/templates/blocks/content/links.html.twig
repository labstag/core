{% use "elements.html.twig" %}
<div{% with {attr: block_contextmenu(block)} %}{{ block('attributes') }}{% endwith %}>
    <div class="wrapper">
        <ul>
            {% for link in links %}
                <li>
                    <a href="{{ link.url }}"{% if link.blank %} target="_blank"{% endif %}{% if link.classes != '' %} class="{{ link.classes }}"{% endif %}>
                        {{ link.title }}
                    </a>
                    {% if link.links is defined and link.links|length != 0 %}
                        <ul>
                            {% for sublink in link.links %}
                                <li>
                                    <a href="{{ sublink.url }}"{% if sublink.blank %} target="_blank"{% endif %}{% if sublink.classes != '' %} class="{{ sublink.classes }}"{% endif %}>
                                        {{ sublink.title }}
                                    </a>
                                </li>
                            {% endfor %}
                        </ul>
                    {% endif %}
                </li>
            {% endfor %}
        </ul>
    </div>
</div>