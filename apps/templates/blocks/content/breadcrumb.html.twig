<nav aria-label="breadcrumb" id="{{ block_id(block) }}" class="{{ block_class(block) }}">
    <div class="wrapper">
        <ul>
            {% for row in urls %}
                <li{% if loop.last %} aria-current="page"{% endif %}>
                    {% if loop.last == false %}
                        <a href="{{ path('front', {'slug': row.url})}}">{{ row.title }}</a>
                    {% else %}
                        {{ row.title }}
                    {% endif %}
                </li>
            {% endfor %}
        </ul>
    </div>
</nav>