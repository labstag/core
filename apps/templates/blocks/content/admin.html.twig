{% use "elements.html.twig" %}
<div{% with {attr: block_contextmenu(block)} %}{{ block('attributes') }}{% endwith %}>
    <ul>
        <li>
            <a href="{{ path('admin') }}" target="_blank">
                {{ 'Admin'|trans }}
            </a>
        </li>
        <li>
            <a href="{{ admin_url('edit', entity) }}" target="_blank">
                {{ 'Edit'|trans }}
            </a>    
        </li>
        <li>
            <a href="{{ admin_url('detail', entity) }}" target="_blank">
                {{ 'Detail'|trans }}
            </a>    
        </li>
        <li>
            <a href="{{ path('app_logout') }}">
                {{ 'Logout'|trans }}
            </a>
        </li>
    </ul>
</div>
<div class="context-menu" id="contextMenu">
    <ul id="menuItems"></ul>
</div>